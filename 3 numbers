# include <stdio.h>
int main()
{
	int n1,n2,n3;
	printf("ENTER THE VALUE OF THREE NUMBERS :\n");
	scanf("%d %d %d",&n1,&n2,&n3);
	if(n1>n2 && n1>n3)
	{
		printf("n1 is greater among all numbers");
	}
	else if(n2>n1 && n2>n3)
	{
		printf("n2 is greatest among all numbers ");
	}
	else if(n3>n1 && n3>n2)
	{
		printf("n3 is greatest among all");
	}
	else
	{
		printf("all are equal");
	}
	return 0;
}
